    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/babel__core": "^7.20.5",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "jest": "^29.7.0",
    "rollup": "^4.29.1",
    "rollup-plugin-import-css": "^3.5.8",
    "ts-jest": "^29.2.5",
    "typescript-eslint": "^8.18.2"
